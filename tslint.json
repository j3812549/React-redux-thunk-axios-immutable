{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "mock/**/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "member-order": false,
    "object-literal-sort-keys": [
      false
    ],
    "no-console": false,
    "jsx-no-lambda": false,
    "no-empty-interface": false,
    "one-variable-per-declaration": false
  }
}